                            <!-- Content -->
                            <h6 class="text-uppercase fw-bold">FoodScape</h6>
                            <hr class="mb-4 mt-0 d-inline-block mx-auto"
                                style="width: 50px; background-color: #7c4dff; height: 2px" />
                            <p>
                                Buy tickets for an amazing summer festival you wouldn't want to miss out on!
                            </p>
                        </div>
                        <!-- Grid column -->
                        <!-- Grid column -->
                        <div class="col-md-3 col-lg-2 col-xl-2 mx-auto mb-4">
