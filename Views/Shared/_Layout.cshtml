<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>@ViewData["Title"] - Food_Scape</title>
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/Food_Scape.styles.css" asp-append-version="true" />
    </head>
    <body class="mb-0">
        <header>
            <nav class="navbar navbar-expand-md navbar-dark box-shadow" style="background-color: rgb(22 28 39)">
                <div class="container-fluid">
                    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="\images\FoodScapeLogo2.png" style="height: 120px;" /></a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                            aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="navbar-collapse collapse justify-content-between" id="navbarNav" style="font-size: 25px;">
                        <ul class="navbar-nav flex" style="margin: 5px;">
                            <li class="nav-item">
                                <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                            </li>
                            <!-- Mobile view: "Explore" dropdown -->
                            <li class="nav-item dropdown d-xl-none d-md-block">
                                <a class="nav-link text-light dropdown-toggle border-0" style="background-color:#f4b139ed; padding: 0.5rem 1rem; border-radius: 0.5rem;" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">Explore</a>
                                <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                    <li style="margin: 5px; font-size: 1.7rem;"><a class="nav-link text-dark dropdown-item" asp-area="" asp-controller="Vendor" asp-action="Index">Vendors</a></li>
                                    <li style="margin: 5px; font-size: 1.7rem;"><a class="nav-link text-dark dropdown-item" asp-area="" asp-controller="TicketType" asp-action="Index">Tickets</a></li>
                                    <li style="margin: 5px; font-size: 1.7rem;"><a class="nav-link text-dark dropdown-item" asp-area="" asp-controller="Event" asp-action="Info">Events</a></li>
                                    <li style="margin: 5px; font-size: 1.7rem;"><a class="nav-link text-dark dropdown-item" asp-area="" asp-controller="Review" asp-action="Index">Reviews</a></li>
                                    <li style="margin: 5px; font-size: 1.7rem;"><a class="nav-link text-dark dropdown-item" asp-area="" asp-controller="Home" asp-action="FAQ">FAQs/Help</a></li>
                                </ul>
                            </li>
                            <!-- Desktop view: "Event", "Vendors", "Ticket Types","Reviews","My Account" navbar items -->
                            <li class="nav-item d-none d-xl-block">
                                <a class="nav-link text-light" asp-area="" asp-controller="Vendor" asp-action="Index">Vendors</a>
                            </li>
                            <li class="nav-item d-none d-xl-block">
                                <a class="nav-link text-light" asp-area="" asp-controller="TicketType" asp-action="Index">
                                    Tickets
                                </a>
                            </li>
                            <li class="nav-item d-none d-xl-block">
                                <a class="nav-link text-light" asp-area="" asp-controller="Event" asp-action="Info">Events</a>
                            </li>
                            <li class="nav-item d-none d-md-none">
                                <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="FAQ">FAQ/Help</a>
                            </li>
                            <li class="nav-item d-none d-xl-block">
                                <a class="nav-link text-light" asp-area="" asp-controller="Review" asp-action="Index">Reviews</a>
                            </li>
                        </ul>
                        <ul class="navbar-nav flex">
                            <partial name="_LoginPartial" />
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
        <div class="min-vh-100">
            @RenderBody()
        </div>
